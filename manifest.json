    "LC_ALL": "C.UTF-8",
    "EDITOR": "micro",
    "FW_UID": "31337"
  },
  "inputs": {
    "api-key": {
      "base": "api-key",
      "read-only": true
    },
    "input": {
      "base": "file",
      "description": "Derivatives to be processed",
      "optional": false
    },
    "QC": {
      "base": "file",
      "description": "Visual QC file from form-parser gear",
      "optional": true
    }
  },
